    "webpack-dev-middleware": "^5.2.1",
    "webpack-dev-server": "^4.3.1"
  },
  "build": {
    "files": [
      "**/*",
      "!**/release-builds/*",
      "!**/docs/*",
      "!**/Apple_Developer_ID_Application_Certificate.p12",
      "!**/notarizeDMG.js",
      "!**/notarizeZIP.js"
    ],
    "extraResources": [
      "Settings.json",
      "WhatsNew.json",
      "py_main",
      "log.ini",
      "db.sqlite3",
      "chart-data",
      "VC_redist.x64.exe"
    ],
    "asarUnpack": [
      "**/node_modules/pdf-to-printer/**/*"
    ],
    "asar": true,
    "extends": null,
    "productName": "Clinic360-Online",
    "afterSign": "./notarizeZIP.js",
    "appId": "com.app.Clinic360Online",
    "mac": {
      "target": [
        "default"
      ],
      "icon": "logo-mac.ico",
      "identity": "35S4B9UGW9",
      "hardenedRuntime": true,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist",
      "extendInfo": {
        "NSMicrophoneUsageDescription": "Please give us access to your microphone"
      }
    },
    "dmg": {
      "artifactName": "${productName}.Setup.${version}.${ext}",
      "sign": true
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "logo-win.ico",
      "requestedExecutionLevel": "highestAvailable"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "artifactName": "${productName}.Setup.${version}.${ext}"
    },
    "directories": {
      "output": "release-builds"
    },
    "publish": [
      {
        "provider": "github",
        "repo": "Clinic360-Online",
        "owner": "Health-360-AI"
      }
    ]
  }
}
